    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/bc_ffffff"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    app:fitType="bottom">


    <com.zjrb.core.ui.widget.web.WebProView
        android:id="@+id/v_web"
        android:layout_width="match_parent"
